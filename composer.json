{
    "name": "mr-luke/privileges",
    "description": "Laravel 5 package that adds roles & priviledges to your application.",
    "keywords": ["eloquent", "privileges", "roles", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Åukasz Sitnicki",
            "email": "lukasz.sitnicki@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^8.0|^7.0|^6.0|^5.8",
        "mr-luke/configuration": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^5.0|^4.0|^3.8.0",
        "phpunit/phpunit": "^8.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mrluke\\Privileges\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrluke\\Privileges\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrluke\\Privileges\\PrivilegesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
